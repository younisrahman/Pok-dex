{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "esnext"
    ],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/api/*": [
        "src/api/*"
      ],
      "@/store/*": [
        "src/store/*"
      ],
      "@/screens/*": [
        "src/screens/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/theme/*": [
        "src/theme/*"
      ],
      "@/navigation/*": [
        "src/navigation/*"
      ]
    }
  },
  "include": [
    "src",
    "app.config.ts",
    "babel.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
